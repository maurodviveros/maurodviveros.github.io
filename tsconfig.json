{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["./components/*"],
      "@consts": ["./consts/index.ts"],
      "@layouts/*": ["./layouts/*"],
      "@types": ["./types"],
      "@i18n": ["./i18n"],
    }
  }
}
